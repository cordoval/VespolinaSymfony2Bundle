    "require": {
        "php": ">=5.3.2",
        "vespolina/core": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Vespolina\\Symfony2Bundle": "" }
    },
    "target-dir": "Vespolina/Symfony2Bundle"
